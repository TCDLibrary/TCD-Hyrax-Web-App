{
  "name": "tcd_hyrax_app_3",
  "private": true,
  "dependencies": {
    "jquery": "3.5.0",
    "lodash": "4.17.21",
    "minimist": "^1.2.3",
    "node": ">=10.0.0",
    "node-fetch": "2.6.7",
    "three": "0.137.0",
    "universalviewer": "^3.1.1",
    "webpack": "^3.0.0",
    "xmldom": "0.6.0"
